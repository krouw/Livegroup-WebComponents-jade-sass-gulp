header.row.between-lg
  .Header.col-lg-12
    .box
      .row.between-lg.middle-lg
        .User.col-lg-4
          .box
            .row
              .User-image.col-lg-2
                img(src="images/50x50.png",class="circle")
              .col-lg
                a(class="User-name dropdown-button",data-activates="User-dropdown") Felipe Rubio
                  i(class="material-icons User-Icon") arrow_drop_down
                p(class="User-empresa") Jumbo La Florida
                ul#User-dropdown.dropdown-content
                  li
                    a Editar Perfil
                  li
                    a Cerrar Sesión
        .Logo.col-lg-4
          img(src="images/150x50.png")
  .Nav.col-lg-12
    .box
      .Nav-Items.row.star-lg
        .col-lg-2
          a.Nav-Item.is-active(href="index.html") Anuncios
